{
  "name": "deel-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/server.js",
    "seed": "node ./scripts/seedDb.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "prettier": "prettier --write '**/*.{js,json,ts}' --ignore-path .eslintignore"
  },
  "pre-commit": [
    "prettier"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "4.17.1",
    "express-async-errors": "^3.1.1",
    "sequelize": "^6.3.4",
    "sqlite3": "^4.0.1"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "jest": "^29.3.1",
    "nodemon": "1.19.1",
    "supertest": "^6.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "test",
    "testRegex": ".test.js$",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "engineStrict": true
}
